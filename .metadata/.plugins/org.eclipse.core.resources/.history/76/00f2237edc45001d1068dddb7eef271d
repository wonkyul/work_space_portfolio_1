package Exception;

public class ExceptoinEx2 {

	public static void main(String[] args) {
		int [] var = {10,200,30};
		for(i = 0 ; i<=3;i++) {
			try {
				System.out.println("var["+i+"] :" + var[i]);
				
			}catch(ArrayIndexOutOfBoundsException ae) {
				System.out.println("배열을 넘었습니다.");
			}
		}//end of for
		System.out.println("프로그램 끝!");

	}

}
